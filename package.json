{
  "name": "@yokotak0527/kensho-rulebook-default",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.es.js",
  "browser": "./dist/bundle.umd.js",
  "exports": {
    "import": "./dist/bundle.es.js",
    "require": "./dist/bundle.cjs.js",
    "node": "./dist/bundle.es.js",
    "browser": "./dist/bundle.umd.js",
    "default": "./dist/bundle.cjs.js"
  },
  "types": "index.d.ts",
  "files": [
    "dist/*",
    "@types/*",
    "index.d.ts",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:es": "cross-env OUTPUT=es rollup -c",
    "build:cjs": "cross-env OUTPUT=cjs rollup -c",
    "build": "npm run build:es && npm run build:cjs",
    "watch": "npm start",
    "lint": "eslint ./",
    "start": "npm run build:es -- -w",
    "clean": "rm -rf ./dist",
    "test": "jest",
    "preversion": "npm run test && npm run lint",
    "version": "npm run clean && npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "validation",
    "TypeScript",
    "Kensho"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yokotak0527/kensho-rulebook-default.git"
  },
  "homepage": "https://github.com/yokotak0527/kensho-rulebook-default#readme",
  "author": "yokotak0527",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@yokotak0527/kensho": "3.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.1.0",
    "jest": "27.3.1",
    "rollup": "2.59.0",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
